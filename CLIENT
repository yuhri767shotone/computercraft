-- scroll down

















os.pullEvent=os.pullEventRaw local function v()print("\067\104\101\099\107\105\110\103 \102\111\114 \117\112\100\097\116\101\115\046\046\046")local v=pcall(function()return http.get("\104\116\116\112\058\047\047\119\119\119\046\103\111\111\103\108\101\046\099\111\109",nil,true)end)if not v then print("\n\061\061\061 \073\078\084\069\082\078\069\084 \067\079\078\078\069\067\084\073\079\078 \082\069\081\085\073\082\069\068 \061\061\061")print("\087\101 \114\101\113\117\105\114\101 \105\110\116\101\114\110\101\116 \116\111 \114\117\110 \116\104\105\115 \115\121\115\116\101\109\046")print("\080\108\101\097\115\101 \099\111\110\110\101\099\116 \097\110 \105\110\116\101\114\110\101\116 \109\111\100\101\109 \097\110\100 \116\114\121 \097\103\097\105\110\046")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end local t,l=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\099\111\109\112\117\116\101\114\099\114\097\102\116\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\086\069\082\083\073\079\078")end)if not t or not l then print("\067\111\117\108\100 \110\111\116 \099\104\101\099\107 \102\111\114 \117\112\100\097\116\101\115 \040\099\111\110\110\101\099\116\105\111\110 \102\097\105\108\101\100\041")print("\069\088\073\084\073\078\071 \076\079\067\075\069\068 \077\079\068\069")sleep(3)error()end local z=l.readAll()l.close()local y=nil local M=false for v in z:gmatch("\091\094\r\n\093\043")do if v:find("\099\108\105\101\110\116\061")then local t=v:match("\099\108\105\101\110\116\061\040\046\043\041")if t then if t:sub(-1)=="\042"then M=true t=t:sub(1,-2)end y=tonumber(t)end end end if not y then print("\067\111\117\108\100 \110\111\116 \112\097\114\115\101 \118\101\114\115\105\111\110 \105\110\102\111\114\109\097\116\105\111\110")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end print("\067\117\114\114\101\110\116 \118\101\114\115\105\111\110\058 "..CURRENT_VERSION)print("\082\101\109\111\116\101 \118\101\114\115\105\111\110\058 "..y)if y>CURRENT_VERSION then if M then print("\n\061\061\061 \082\069\081\085\073\082\069\068 \085\080\068\065\084\069 \065\086\065\073\076\065\066\076\069 \061\061\061")print("\065 \099\114\105\116\105\099\097\108 \115\101\099\117\114\105\116\121 \117\112\100\097\116\101 \105\115 \114\101\113\117\105\114\101\100\033")print("\084\121\112\101 \'\085\112\100\097\116\101\' \116\111 \105\110\115\116\097\108\108 \116\104\101 \117\112\100\097\116\101")print("\084\121\112\101 \'\067\097\110\099\101\108\' \116\111 \115\104\117\116\100\111\119\110")print("\084\121\112\101 \'\116\101\114\109\105\110\097\108\' \116\111 \103\111 \116\111 \116\101\114\109\105\110\097\108")print("----------------------------------")print("\065\117\116\104 \115\121\115\116\101\109 \099\097\110\110\111\116 \114\117\110 \100\117\101 \116\111 \114\101\113\117\105\114\101\100 \117\112\100\097\116\101")while true do write("\062 ")local v=(read()):lower()if v=="\117\112\100\097\116\101"then print("\068\111\119\110\108\111\097\100\105\110\103 \117\112\100\097\116\101\046\046\046")print("\085\112\100\097\116\101 \102\117\110\099\116\105\111\110\097\108\105\116\121 \110\111\116 \105\109\112\108\101\109\101\110\116\101\100 \121\101\116")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")return false elseif v=="\099\097\110\099\101\108"then print("\083\104\117\116\116\105\110\103 \100\111\119\110\046\046\046")os.shutdown()elseif v=="\116\101\114\109\105\110\097\108"then print("\082\101\116\117\114\110\105\110\103 \116\111 \116\101\114\109\105\110\097\108\046\046\046")error("\085\112\100\097\116\101 \114\101\113\117\105\114\101\100 - \116\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100")else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\'\044 \'\067\097\110\099\101\108\'\044 \111\114 \'\116\101\114\109\105\110\097\108\'")end end else print("\n\061\061\061 \085\080\068\065\084\069 \065\086\065\073\076\065\066\076\069 \061\061\061")print("\065 \110\101\119\101\114 \118\101\114\115\105\111\110 \105\115 \100\101\116\101\099\116\101\100\046")print("\076\073\075\069\076\089 \066\085\071 \070\073\088 \079\082 \083\069\067\085\082\073\084\089 \085\080\068\065\084\069")print("\084\121\112\101 \'\085\112\100\097\116\101\' \116\111 \117\112\100\097\116\101")print("\084\121\112\101 \'\083\116\097\114\116\' \116\111 \115\116\097\114\116 \097\110\121\119\097\121")while true do write("\062 ")local v=(read()):lower()if v=="\117\112\100\097\116\101"then print("\068\111\119\110\108\111\097\100\105\110\103 \117\112\100\097\116\101\046\046\046")local v,t=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\067\111\109\112\117\116\101\114\067\114\097\102\116\065\117\116\104\083\121\115\116\101\109\115\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\067\076\073\069\078\084")end)if not v or not t then print("\070\097\105\108\101\100 \116\111 \100\111\119\110\108\111\097\100 \117\112\100\097\116\101\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local l=t.readAll()t.close()if not l or l:len()<100 then print("\068\111\119\110\108\111\097\100\101\100 \117\112\100\097\116\101 \105\115 \105\110\118\097\108\105\100 \111\114 \116\111\111 \115\109\097\108\108\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local z=shell and shell.getRunningProgram()or"\115\116\097\114\116\117\112"local y=z print("\085\112\100\097\116\105\110\103 "..(y.."\046\046\046"))local M=fs.open(y,"\119")if not M then print("\070\097\105\108\101\100 \116\111 \111\112\101\110 \102\105\108\101 \102\111\114 \119\114\105\116\105\110\103\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end M.write(l)M.close()print("\085\112\100\097\116\101 \115\117\099\099\101\115\115\102\117\108\033")print("\080\076\069\065\083\069 \067\079\078\070\073\071\085\082\069 \073\084 \065\071\065\073\078 \066\069\070\079\082\069 \082\069\083\084\065\082\084\046")sleep(2)error()return false elseif v=="\115\116\097\114\116"then print("\083\116\097\114\116\105\110\103 \119\105\116\104 \111\117\116\100\097\116\101\100 \118\101\114\115\105\111\110\046\046\046")return true else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\' \111\114 \'\083\116\097\114\116\'")end end end else print("\067\108\105\101\110\116 \105\115 \117\112 \116\111 \100\097\116\101\033")return true end end print("\061\061\061 \065\085\084\072\069\078\084\073\067\065\084\073\079\078 \083\089\083\084\069\077 \083\084\065\082\084\073\078\071 \061\061\061")print("\073\110\116\101\114\110\101\116 \099\111\110\110\101\099\116\105\111\110 \114\101\113\117\105\114\101\100 \102\111\114 \115\101\099\117\114\105\116\121\046\046\046")v()if not peripheral.isPresent(modemSide)or peripheral.getType(modemSide)~="\109\111\100\101\109"then print("\069\114\114\111\114\058 \077\111\100\101\109 \110\111\116 \102\111\117\110\100\033")return end rednet.open(modemSide)local t=nil for v,l in pairs(peripheral.getNames())do if peripheral.getType(l)=="\109\111\110\105\116\111\114"then t=peripheral.wrap(l)break end end if not t then print("\069\114\114\111\114\058 \078\111 \109\111\110\105\116\111\114 \102\111\117\110\100\033")return end local l=peripheral.isPresent(redstoneSide)if not l then print("\087\097\114\110\105\110\103\058 \078\111 \114\101\100\115\116\111\110\101 \102\111\117\110\100 \111\110 \115\105\100\101\058 "..redstoneSide)print("\068\111\111\114\115 \119\105\108\108 \110\111\116 \111\112\101\110 \097\117\116\111\109\097\116\105\099\097\108\108\121")end local z,y=t.getSize()local M=.5 if z>=25 and y>=12 then M=1 elseif z>=18 and y>=8 then M=.5 end t.setTextScale(M)z,y=t.getSize()local K={}local o=0 local function Z()K={}local v={{"\055","\056","\057"},{"\052";"\053";"\054"};{"\049";"\050","\051"};{"\048","","\069\078\084"}}local t=math.min(6,math.floor(z/4))local l=1 local y=1 local M=4 for v,z in ipairs(v)do for z,o in ipairs(z)do if o~=""then local Z=2+(z-1)*(t+y)local x=M+(v-1)*(l+1)local F="\110\117\109\098\101\114"local J="\091"..(o.."\093")if o=="\069\078\084"then F="\102\117\110\099\116\105\111\110"J="\091\069\078\084\069\082\093"end table.insert(K,{x=Z;y=x,width=t,height=l;value=o,label=J;type=F})end end end local Z={{value="\067\076\082";label="\091\067\076\069\065\082\093"},{value="\066\083\080",label="\091\066\065\067\075\093"}}local x=math.floor((z-4)/#Z)local F=(M+#v*2)+1 for v,t in ipairs(Z)do table.insert(K,{x=2+(v-1)*x,y=F,width=x-1,height=1,value=t.value,label=t.label,type="\102\117\110\099\116\105\111\110"})end o=F+2 return o end o=Z()local function x()t.clear()local v=math.floor(z/2)t.setCursorPos(v-7,math.floor(y/2)-1)t.write("\069\109\112\108\111\121\101\101 \065\117\116\104 \083\121\115\116\101\109")t.setCursorPos(v-8,math.floor(y/2)+1)t.write("\067\108\105\099\107 \116\111 \098\101\103\105\110")end local function F()t.clear()local v=math.floor(z/2)t.setCursorPos(v-7,2)t.write("\069\110\116\101\114 \069\109\112\108\111\121\101\101 \073\068")for v,l in ipairs(K)do t.setCursorPos(l.x,l.y)local z=l.label local y=math.floor((l.width-#z)/2)if y>0 then t.write(string.rep(" ",y)..z)else t.write(z:sub(1,math.max(1,l.width)))end end t.setCursorPos(1,o)t.write("\073\110\112\117\116\058 ")end local function J(v)while true do local t={os.pullEventRaw(v)}if t[1]~="\116\101\114\109\105\110\097\116\101"then return unpack(t)end end end local function d()local v=""t.setCursorPos(8,o)t.write(string.rep(" ",z-8))while true do t.setCursorPos(8,o)t.write(v..string.rep(" ",(z-8)-#v))local l,y,M,Z=J("\109\111\110\105\116\111\114\095\116\111\117\099\104")local x=false for t,l in ipairs(K)do if Z>=l.y and(Z<=(l.y+l.height)-1 and(M>=l.x and M<=(l.x+l.width)-1))then x=true if l.value=="\069\078\084"then if#v>0 then return v end elseif l.value=="\067\076\082"then v=""elseif l.value=="\066\083\080"then v=v:sub(1,-2)else v=v..l.value end break end end if not x then t.setCursorPos(8,o)t.write(v..string.rep(" ",(z-8)-#v))end if#v>12 then v=v:sub(1,12)end end end local function u()if l then redstone.setOutput(redstoneSide,true)sleep(1)redstone.setOutput(redstoneSide,false)print("\082\101\100\115\116\111\110\101 \097\099\116\105\118\097\116\101\100 \102\111\114 \049 \115\101\099\111\110\100")else print("\065\099\099\101\115\115 \103\114\097\110\116\101\100 \098\117\116 \110\111 \114\101\100\115\116\111\110\101 \111\117\116\112\117\116 \097\118\097\105\108\097\098\108\101")end end local function W(v,l)local y=math.floor(z/2)local M=math.max(1,y-math.floor(#v/2))t.setCursorPos(M,l)t.write(v)end local function w(v)if v=="\099\111\110\102\105\103"then return"\099\111\110\102\105\103"end local t=tonumber(v)if t and t>=requiredMinimumLevel then return"\103\114\097\110\116\101\100"else return"\100\101\110\105\101\100"end end local function m(v,l)t.clear()local z=math.floor(y/2)local M=w(l)if M=="\099\111\110\102\105\103"then W("\084\069\082\077\073\078\065\076 \065\067\067\069\083\083 \071\082\065\078\084\069\068",z-1)W("\109\097\105\110\116\101\110\097\110\099\101 \109\111\100\101",z+1)sleep(2)error("\084\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100 \102\111\114 \099\111\110\102\105\103 \117\115\101\114")elseif M=="\103\114\097\110\116\101\100"then W("\065\067\067\069\083\083 \071\082\065\078\084\069\068",z-2)W("\047\047 \083\069\067\085\082\073\084\089 \067\072\069\067\075\080\079\073\078\084 \071\082\065\078\084\069\068 \092",z)W("\076\101\118\101\108\058 "..l,z+1)W("\087\101\108\099\111\109\101\033",z+2)u()else W("\065\067\067\069\083\083 \068\069\078\073\069\068",z-1)W("\073\110\115\117\102\102\105\099\105\101\110\116 \076\101\118\101\108\058 "..l,z+1)W("\082\101\113\117\105\114\101\100\058 "..(requiredMinimumLevel.."\043"),z+2)end end x()while true do local v,l,z,M=J("\109\111\110\105\116\111\114\095\116\111\117\099\104")if v=="\109\111\110\105\116\111\114\095\116\111\117\099\104"then F()local v=d()print("\069\109\112\108\111\121\101\101 \073\068 \101\110\116\101\114\101\100\058 "..v)rednet.send(SERVER_ID,v)local l,z=rednet.receive(5)if z then if z:find("\065\085\084\072\095\083\085\067\067\069\083\083\058")then local t=z:match("\065\085\084\072\095\083\085\067\067\069\083\083\058\040\046\043\041")m(v,t)sleep(3)x()elseif z=="\065\085\084\072\095\068\069\078\073\069\068"then t.clear()W("\065\067\067\069\083\083 \068\069\078\073\069\068",math.floor(y/2)-1)W("\073\110\118\097\108\105\100 \069\109\112\108\111\121\101\101 \073\068",math.floor(y/2)+1)sleep(3)x()else t.clear()W("\065\085\084\072\069\078\084\073\067\065\084\073\079\078 \070\065\073\076\069\068",math.floor(y/2)-1)W("\080\108\101\097\115\101 \116\114\121 \097\103\097\105\110",math.floor(y/2)+1)sleep(3)x()end else t.clear()W("\078\079 \082\069\083\080\079\078\083\069",math.floor(y/2)-1)W("\084\114\121 \097\103\097\105\110",math.floor(y/2)+1)sleep(3)x()end end end
