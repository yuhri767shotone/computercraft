local x=2 os.pullEvent=os.pullEventRaw local function D()print("\067\104\101\099\107\105\110\103 \102\111\114 \117\112\100\097\116\101\115\046\046\046")local D=pcall(function()return http.get("\104\116\116\112\058\047\047\119\119\119\046\103\111\111\103\108\101\046\099\111\109",nil,true)end)if not D then print("\n\061\061\061 \073\078\084\069\082\078\069\084 \067\079\078\078\069\067\084\073\079\078 \082\069\081\085\073\082\069\068 \061\061\061")print("\087\101 \114\101\113\117\105\114\101 \105\110\116\101\114\110\101\116 \116\111 \114\117\110 \116\104\105\115 \115\121\115\116\101\109\046")print("\080\108\101\097\115\101 \099\111\110\110\101\099\116 \097\110 \105\110\116\101\114\110\101\116 \109\111\100\101\109 \097\110\100 \116\114\121 \097\103\097\105\110\046")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end local l,j=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\099\111\109\112\117\116\101\114\099\114\097\102\116\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\086\069\082\083\073\079\078")end)if not l or not j then print("\067\111\117\108\100 \110\111\116 \099\104\101\099\107 \102\111\114 \117\112\100\097\116\101\115 \040\099\111\110\110\101\099\116\105\111\110 \102\097\105\108\101\100\041")print("\069\088\073\084\073\078\071 \076\079\067\075\069\068 \077\079\068\069")sleep(3)error()end local v=j.readAll()j.close()local K=nil local c=false for x in v:gmatch("\091\094\r\n\093\043")do if x:find("\099\108\105\101\110\116\061")then local D=x:match("\099\108\105\101\110\116\061\040\046\043\041")if D then if D:sub(-1)=="\042"then c=true D=D:sub(1,-2)end K=tonumber(D)end end end if not K then print("\067\111\117\108\100 \110\111\116 \112\097\114\115\101 \118\101\114\115\105\111\110 \105\110\102\111\114\109\097\116\105\111\110")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end print("\067\117\114\114\101\110\116 \118\101\114\115\105\111\110\058 "..x)print("\082\101\109\111\116\101 \118\101\114\115\105\111\110\058 "..K)if K>x then if c then print("\n\061\061\061 \082\069\081\085\073\082\069\068 \085\080\068\065\084\069 \065\086\065\073\076\065\066\076\069 \061\061\061")print("\065 \099\114\105\116\105\099\097\108 \115\101\099\117\114\105\116\121 \117\112\100\097\116\101 \105\115 \114\101\113\117\105\114\101\100\033")print("\084\121\112\101 \'\085\112\100\097\116\101\' \116\111 \105\110\115\116\097\108\108 \116\104\101 \117\112\100\097\116\101")print("\084\121\112\101 \'\067\097\110\099\101\108\' \116\111 \115\104\117\116\100\111\119\110")print("\084\121\112\101 \'\116\101\114\109\105\110\097\108\' \116\111 \103\111 \116\111 \116\101\114\109\105\110\097\108")print("----------------------------------")print("\065\117\116\104 \115\121\115\116\101\109 \099\097\110\110\111\116 \114\117\110 \100\117\101 \116\111 \114\101\113\117\105\114\101\100 \117\112\100\097\116\101")while true do write("\062 ")local x=(read()):lower()if x=="\117\112\100\097\116\101"then print("\068\111\119\110\108\111\097\100\105\110\103 \117\112\100\097\116\101\046\046\046")local x,D=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\099\111\109\112\117\116\101\114\099\114\097\102\116\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\067\076\073\069\078\084")end)if not x or not D then print("\070\097\105\108\101\100 \116\111 \100\111\119\110\108\111\097\100 \117\112\100\097\116\101\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local l=D.readAll()D.close()if not l or l:len()<100 then print("\068\111\119\110\108\111\097\100\101\100 \117\112\100\097\116\101 \105\115 \105\110\118\097\108\105\100 \111\114 \116\111\111 \115\109\097\108\108\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local j=shell and shell.getRunningProgram()or"\115\116\097\114\116\117\112"local v=j print("\085\112\100\097\116\105\110\103 "..(v.."\046\046\046"))local K=fs.open(v,"\119")if not K then print("\070\097\105\108\101\100 \116\111 \111\112\101\110 \102\105\108\101 \102\111\114 \119\114\105\116\105\110\103\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end K.write(l)K.close()print("\085\112\100\097\116\101 \115\117\099\099\101\115\115\102\117\108\033")print("\080\076\069\065\083\069 \067\079\078\070\073\071\085\082\069 \073\084 \065\071\065\073\078 \066\069\070\079\082\069 \082\069\083\084\065\082\084\046")print("\082\101\115\116\097\114\116\105\110\103 \105\110 \051 \115\101\099\111\110\100\115\046\046\046")sleep(3)os.reboot()return false elseif x=="\099\097\110\099\101\108"then print("\083\104\117\116\116\105\110\103 \100\111\119\110\046\046\046")os.shutdown()elseif x=="\116\101\114\109\105\110\097\108"then print("\082\101\116\117\114\110\105\110\103 \116\111 \116\101\114\109\105\110\097\108\046\046\046")error("\085\112\100\097\116\101 \114\101\113\117\105\114\101\100 - \116\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100")else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\'\044 \'\067\097\110\099\101\108\'\044 \111\114 \'\116\101\114\109\105\110\097\108\'")end end else print("\n\061\061\061 \085\080\068\065\084\069 \065\086\065\073\076\065\066\076\069 \061\061\061")print("\065 \110\101\119\101\114 \118\101\114\115\105\111\110 \105\115 \100\101\116\101\099\116\101\100\046")print("\076\073\075\069\076\089 \066\085\071 \070\073\088 \079\082 \083\069\067\085\082\073\084\089 \085\080\068\065\084\069")print("\084\121\112\101 \'\085\112\100\097\116\101\' \116\111 \117\112\100\097\116\101")print("\084\121\112\101 \'\083\116\097\114\116\' \116\111 \115\116\097\114\116 \097\110\121\119\097\121")while true do write("\062 ")local x=(read()):lower()if x=="\117\112\100\097\116\101"then print("\068\111\119\110\108\111\097\100\105\110\103 \117\112\100\097\116\101\046\046\046")local x,D=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\099\111\109\112\117\116\101\114\099\114\097\102\116\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\067\076\073\069\078\084")end)if not x or not D then print("\070\097\105\108\101\100 \116\111 \100\111\119\110\108\111\097\100 \117\112\100\097\116\101\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local l=D.readAll()D.close()if not l or l:len()<100 then print("\068\111\119\110\108\111\097\100\101\100 \117\112\100\097\116\101 \105\115 \105\110\118\097\108\105\100 \111\114 \116\111\111 \115\109\097\108\108\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local j=shell and shell.getRunningProgram()or"\115\116\097\114\116\117\112"local v=j print("\085\112\100\097\116\105\110\103 "..(v.."\046\046\046"))local K=fs.open(v,"\119")if not K then print("\070\097\105\108\101\100 \116\111 \111\112\101\110 \102\105\108\101 \102\111\114 \119\114\105\116\105\110\103\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end K.write(l)K.close()print("\085\112\100\097\116\101 \115\117\099\099\101\115\115\102\117\108\033")print("\080\076\069\065\083\069 \067\079\078\070\073\071\085\082\069 \073\084 \065\071\065\073\078 \066\069\070\079\082\069 \082\069\083\084\065\082\084\046")print("\082\101\115\116\097\114\116\105\110\103 \105\110 \051 \115\101\099\111\110\100\115\046\046\046")sleep(3)os.reboot()return false elseif x=="\115\116\097\114\116"then print("\083\116\097\114\116\105\110\103 \119\105\116\104 \111\117\116\100\097\116\101\100 \118\101\114\115\105\111\110\046\046\046")return true else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\' \111\114 \'\083\116\097\114\116\'")end end end else print("\067\108\105\101\110\116 \105\115 \117\112 \116\111 \100\097\116\101\033")return true end end print("\061\061\061 \065\085\084\072\069\078\084\073\067\065\084\073\079\078 \083\089\083\084\069\077 \083\084\065\082\084\073\078\071 \061\061\061")print("\073\110\116\101\114\110\101\116 \099\111\110\110\101\099\116\105\111\110 \114\101\113\117\105\114\101\100 \102\111\114 \115\101\099\117\114\105\116\121\046\046\046")D()if not peripheral.isPresent(modemSide)or peripheral.getType(modemSide)~="\109\111\100\101\109"then print("\069\114\114\111\114\058 \077\111\100\101\109 \110\111\116 \102\111\117\110\100\033")return end rednet.open(modemSide)local l=nil for x,D in pairs(peripheral.getNames())do if peripheral.getType(D)=="\109\111\110\105\116\111\114"then l=peripheral.wrap(D)break end end if not l then print("\069\114\114\111\114\058 \078\111 \109\111\110\105\116\111\114 \102\111\117\110\100\033")return end local j=peripheral.isPresent(redstoneSide)if not j then print("\087\097\114\110\105\110\103\058 \078\111 \114\101\100\115\116\111\110\101 \102\111\117\110\100 \111\110 \115\105\100\101\058 "..redstoneSide)print("\087\105\108\108 \097\116\116\101\109\112\116 \114\101\100\115\116\111\110\101 \111\117\116\112\117\116 \097\110\121\119\097\121\046\046\046")end local v,K=l.getSize()local c=.5 if v>=25 and K>=12 then c=1 elseif v>=18 and K>=8 then c=.5 end l.setTextScale(c)v,K=l.getSize()local F={}local N=0 local function P()F={}local x={{"\055","\056";"\057"},{"\052";"\053","\054"};{"\049","\050";"\051"},{"\048";"";"\069\078\084"}}local D=math.min(6,math.floor(v/4))local l=1 local j=1 local K=4 for x,v in ipairs(x)do for v,c in ipairs(v)do if c~=""then local N=2+(v-1)*(D+j)local P=K+(x-1)*(l+1)local s="\110\117\109\098\101\114"local B="\091"..(c.."\093")if c=="\069\078\084"then s="\102\117\110\099\116\105\111\110"B="\091\069\078\084\069\082\093"end table.insert(F,{x=N;y=P;width=D;height=l,value=c,label=B;type=s})end end end local c={{value="\067\076\082",label="\091\067\076\069\065\082\093"};{value="\066\083\080",label="\091\066\065\067\075\093"}}local P=math.floor((v-4)/#c)local s=(K+#x*2)+1 for x,D in ipairs(c)do table.insert(F,{x=2+(x-1)*P,y=s;width=P-1;height=1,value=D.value,label=D.label,type="\102\117\110\099\116\105\111\110"})end N=s+2 return N end N=P()local function s()l.clear()local x=math.floor(v/2)l.setCursorPos(x-7,math.floor(K/2)-1)l.write("\069\109\112\108\111\121\101\101 \065\117\116\104 \083\121\115\116\101\109")l.setCursorPos(x-8,math.floor(K/2)+1)l.write("\067\108\105\099\107 \116\111 \098\101\103\105\110")end local function B()l.clear()local x=math.floor(v/2)l.setCursorPos(x-7,2)l.write("\069\110\116\101\114 \069\109\112\108\111\121\101\101 \073\068")for x,D in ipairs(F)do l.setCursorPos(D.x,D.y)local j=D.label local v=math.floor((D.width-#j)/2)if v>0 then l.write(string.rep(" ",v)..j)else l.write(j:sub(1,math.max(1,D.width)))end end l.setCursorPos(1,N)l.write("\073\110\112\117\116\058 ")end local function u(x)while true do local D={os.pullEventRaw(x)}if D[1]~="\116\101\114\109\105\110\097\116\101"then return unpack(D)end end end local function y()local x=""l.setCursorPos(8,N)l.write(string.rep(" ",v-8))while true do l.setCursorPos(8,N)l.write(x..string.rep(" ",(v-8)-#x))local D,j,K,c=u("\109\111\110\105\116\111\114\095\116\111\117\099\104")local P=false for D,l in ipairs(F)do if c>=l.y and(c<=(l.y+l.height)-1 and(K>=l.x and K<=(l.x+l.width)-1))then P=true if l.value=="\069\078\084"then if#x>0 then return x end elseif l.value=="\067\076\082"then x=""elseif l.value=="\066\083\080"then x=x:sub(1,-2)else x=x..l.value end break end end if not P then l.setCursorPos(8,N)l.write(x..string.rep(" ",(v-8)-#x))end if#x>12 then x=x:sub(1,12)end end end local function U()local x=pcall(function()redstone.setOutput(redstoneSide,true)sleep(1)redstone.setOutput(redstoneSide,false)end)if x then print("\082\101\100\115\116\111\110\101 \097\099\116\105\118\097\116\101\100 \102\111\114 \049 \115\101\099\111\110\100")else print("\082\101\100\115\116\111\110\101 \097\099\116\105\118\097\116\105\111\110 \097\116\116\101\109\112\116\101\100 \098\117\116 \102\097\105\108\101\100")end end local function H(x,D)local j=math.floor(v/2)local K=math.max(1,j-math.floor(#x/2))l.setCursorPos(K,D)l.write(x)end local function W(x)if x=="\099\111\110\102\105\103"then return"\099\111\110\102\105\103"end local D=tonumber(x)if D and D>=requiredMinimumLevel then return"\103\114\097\110\116\101\100"else return"\100\101\110\105\101\100"end end local function a(x,D)l.clear()local j=math.floor(K/2)local v=W(D)if v=="\099\111\110\102\105\103"then H("\084\069\082\077\073\078\065\076 \065\067\067\069\083\083 \071\082\065\078\084\069\068",j-1)H("\109\097\105\110\116\101\110\097\110\099\101 \109\111\100\101",j+1)sleep(2)error("\084\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100 \102\111\114 \099\111\110\102\105\103 \117\115\101\114")elseif v=="\103\114\097\110\116\101\100"then H("\065\067\067\069\083\083 \071\082\065\078\084\069\068",j-2)H("\047\047 \083\069\067\085\082\073\084\089 \067\072\069\067\075\080\079\073\078\084 \071\082\065\078\084\069\068 \092",j)H("\076\101\118\101\108\058 "..D,j+1)H("\087\101\108\099\111\109\101\033",j+2)U()else H("\065\067\067\069\083\083 \068\069\078\073\069\068",j-1)H("\073\110\115\117\102\102\105\099\105\101\110\116 \076\101\118\101\108\058 "..D,j+1)H("\082\101\113\117\105\114\101\100\058 "..(requiredMinimumLevel.."\043"),j+2)end end s()while true do local x,D,j,v=u("\109\111\110\105\116\111\114\095\116\111\117\099\104")if x=="\109\111\110\105\116\111\114\095\116\111\117\099\104"then B()local x=y()print("\069\109\112\108\111\121\101\101 \073\068 \101\110\116\101\114\101\100\058 "..x)rednet.send(SERVER_ID,x)local D,j=rednet.receive(5)if j then if j:find("\065\085\084\072\095\083\085\067\067\069\083\083\058")then local D=j:match("\065\085\084\072\095\083\085\067\067\069\083\083\058\040\046\043\041")a(x,D)sleep(3)s()elseif j=="\065\085\084\072\095\068\069\078\073\069\068"then l.clear()H("\065\067\067\069\083\083 \068\069\078\073\069\068",math.floor(K/2)-1)H("\073\110\118\097\108\105\100 \069\109\112\108\111\121\101\101 \073\068",math.floor(K/2)+1)sleep(3)s()else l.clear()H("\065\085\084\072\069\078\084\073\067\065\084\073\079\078 \070\065\073\076\069\068",math.floor(K/2)-1)H("\080\108\101\097\115\101 \116\114\121 \097\103\097\105\110",math.floor(K/2)+1)sleep(3)s()end else l.clear()H("\078\079 \082\069\083\080\079\078\083\069",math.floor(K/2)-1)H("\084\114\121 \097\103\097\105\110",math.floor(K/2)+1)sleep(3)s()end end end
