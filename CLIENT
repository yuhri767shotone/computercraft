local w=1 os.pullEvent=os.pullEventRaw local function o()print("\067\104\101\099\107\105\110\103 \102\111\114 \117\112\100\097\116\101\115\046\046\046")local o=pcall(function()return http.get("\104\116\116\112\058\047\047\119\119\119\046\103\111\111\103\108\101\046\099\111\109",nil,true)end)if not o then print("\n\061\061\061 \073\078\084\069\082\078\069\084 \067\079\078\078\069\067\084\073\079\078 \082\069\081\085\073\082\069\068 \061\061\061")print("\087\101 \114\101\113\117\105\114\101 \105\110\116\101\114\110\101\116 \116\111 \114\117\110 \116\104\105\115 \115\121\115\116\101\109\046")print("\080\108\101\097\115\101 \099\111\110\110\101\099\116 \097\110 \105\110\116\101\114\110\101\116 \109\111\100\101\109 \097\110\100 \116\114\121 \097\103\097\105\110\046")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end local K,C=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\099\111\109\112\117\116\101\114\099\114\097\102\116\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\086\069\082\083\073\079\078")end)if not K or not C then print("\067\111\117\108\100 \110\111\116 \099\104\101\099\107 \102\111\114 \117\112\100\097\116\101\115 \040\099\111\110\110\101\099\116\105\111\110 \102\097\105\108\101\100\041")print("\069\088\073\084\073\078\071 \076\079\067\075\069\068 \077\079\068\069")sleep(3)error()end local t=C.readAll()C.close()local F=nil local c=false for w in t:gmatch("\091\094\r\n\093\043")do if w:find("\099\108\105\101\110\116\061")then local o=w:match("\099\108\105\101\110\116\061\040\046\043\041")if o then if o:sub(-1)=="\042"then c=true o=o:sub(1,-2)end F=tonumber(o)end end end if not F then print("\067\111\117\108\100 \110\111\116 \112\097\114\115\101 \118\101\114\115\105\111\110 \1