 end local C=shell and shell.getRunningProgram()or"\115\116\097\114\116\117\112"local t=C print("\085\112\100\097\116\105\110\103 "..(t.."\046\046\046"))local F=fs.open(t,"\119")if not F then print("\070\097\105\108\101\100 \116\111 \111\112\101\110 \102\105\108\101 \102\111\114 \119\114\105\116\105\110\103\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end F.write(K)F.close()print("\085\112\100\097\116\101 \115\117\099\099\101\115\115\102\117\108\033")print("\080\076\069\065\083\069 \067\079\078\070\073\071\085\082\069 \073\084 \065\071\065\073\078 \066\069\070\079\082\069 \082\069\083\084\065\082\084\046")sleep(2)error()return false elseif w=="\115\116\097\114\116"then print("\083\116\097\114\116\105\110\103 \119\105\116\104 \111\117\116\100\097\116\101\100 \118\101\114\115\105\111\110\046\046\046")return true else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\' \111\114 \'\083\116\097\114\116\'")end end end else print("\067\108\105\101\110\116 \105\115 \117\112 \116\111 \100\097\116\101\033")return true end end print("\061\061\061 \065\085\084\072\069\078\084\073\067\065\084\073\079\078 \083\089\083\084\069\077 \083\084\065\082\084\073\078\071 \061\061\061")print("\073\110\116\101\114\110\101\116 \099\111\110\110\101\099\116\105\111\110 \114\101\113\117\105\114\101\100 \102\111\114 \115\101\099\117\114\105\116\121\046\046\046")o()if not peripheral.isPresent(modemSide)or peripheral.getType(modemSide)~="\109\111\100\101\109"then print("\069\114\114\111\114\058 \077\111\100\101\109 \110\111\116 \102\111\117\110\100\033")return end rednet.open(modemSide)local K=nil for w,o in pairs(peripheral.getNames())do if peripheral.getType(o)=="\109\111\110\105\116\111\114"then K=peripheral.wrap(o)break end end if not K then print("\069\114\114\111\114\058 \078\111 \109\111\110\105\116\111\114 \102\111\117\110\100\033")return end local C=peripheral.isPresent(redstoneSide)if not C then print("\087\097\114\110\105\110\103\058 \078\111 \114\101\100\115\116\111\110\101 \102\111\117\110\100 \111\110 \115\105\100\101\058 "..redstoneSide)print("\068\111\111\114\115 \119\105\108\108 \110\111\116 \111\112\101\110 \097\117\116\111\109\097\116\105\099\097\108\108\121")end local t,F=K.getSize()local c=.5 if t>=25 and F>=12 then c=1 elseif t>=18 and F>=8 then c=.5 end K.setTextScale(c)t,F=K.getSize()local k={}local h=0 local function N()k={}local w={{"\055";"\056";"\057"},{"\052","\053";"\054"},{"\049","\050";"\051"};{"\048";"","\069\078\084"}}local o=math.min(6,math.floor(t/4))local K=1 local C=1 local F=4 for w,t in ipairs(w)do for t,c in ipairs(t)do if c~=""then local h=2+(t-1)*(o+C)local N=F+(w-1)*(K+1)local y="\110\117\109\098\101\114"local H="\091"..(c.."\093")if c=="\069\078\084"then y="\102\117\110\099\116\105\111\110"H="\091\069\078\084\069\082\093"end table.insert(k,{x=h;y=N;width=o,height=K,value=c,label=H;type=y})end end end local c={{value="\067\076\082",label="\091\067\076\069\065\082\093"};{value="\066\083\080",label="\091\066\065\067\075\093"}}local N=math.floor((t-4)/#c)local y=(F+#w*2)+1 for w,o in ipairs(c)do table.insert(k,{x=2+(w-1)*N;y=y,width=N-1,height=1,value=o.value,label=o.label,type="\102\117\110\099\116\105\111\110"})end h=y+2 return h end h=N()local function y()K.clear()local w=math.floor(t/2)K.setCursorPos(w-7,math.floor(F/2)-1)K.write("\069\109\112\108\111\121\101\101 \065\117\116\104 \083\121\115\116\101\109")K.setCursorPos(w-8,math.floor(F/2)+1)K.write("\067\108\105\099\107 \116\111 \098\101\103\105\110")end local function H()K.clear()local w=math.floor(t/2)K.setCursorPos(w-7,2)K.write("\069\110\116\101\114 \069\109\112\108\111\121\101\101 \073\068")for w,o in ipairs(k)do K.setCursorPos(o.x,o.y)local C=o.label local t=math.floor((o.width-#C)/2)if t>0 then K.write(string.rep(" ",t)..C)else K.write(C:sub(1,math.max(1,o.width)))end end K.setCursorPos(1,h)K.write("\073\110\112\117\116\058 ")end local function s(w)while true do local o={os.pullEventRaw(w)}if o[1]~="\116\101\114\109\105\110\097\116\101"then return unpack(o)end end end local function X()local w=""K.setCursorPos(8,h)K.write(string.rep(" ",t-8))while true do K.setCursorPos(8,h)K.write(w..string.rep(" ",(t-8)-#w))local o,C,F,c=s("\109\111\110\105\116\111\114\095\116\111\117\099\104")local N=false for o,K in ipairs(k)do if c>=K.y and(c<=(K.y+K.height)-1 and(F>=K.x and F<=(K.x+K.width)-1))then N=true if K.value=="\069\078\084"then if#w>0 then return w end elseif K.value=="\067\076\082"then w=""elseif K.value=="\066\083\080"then w=w:sub(1,-2)else w=w..K.value end break end end if not N then K.setCursorPos(8,h)K.write(w..string.rep(" ",(t-8)-#w))end if#w>12 then w=w:sub(1,12)end end end local function j()if C then redstone.setOutput(redstoneSide,true)sleep(1)redstone.setOutput(redstoneSide,false)print("\082\101\100\115\116\111\110\101 \097\099\116\105\118\097\116\101\100 \102\111\114 \049 \115\101\099\111\110\100")else print("\065\099\099\101\115\115 \103\114\097\110\116\101\100 \098\117\116 \110\111 \114\101\100\115\116\111\110\101 \111\117\116\112\117\116 \097\118\097\105\108\097\098\108\101")end end local function m(w,o)local C=math.floor(t/2)local F=math.max(1,C-math.floor(#w/2))K.setCursorPos(F,o)K.write(w)end local function O(w)if w=="\099\111\110\102\105\103"then return"\099\111\110\102\105\103"end local o=tonumber(w)if o and o>=requiredMinimumLevel then return"\103\114\097\110\116\101\100"else return"\100\101\110\105\101\100"end end local function b(w,o)K.clear()local C=math.floor(F/2)local t=O(o)if t=="\099\111\110\102\105\103"then m("\084\069\082\077\073\078\065\076 \065\067\067\069\083\083 \071\082\065\078\084\069\068",C-1)m("\109\097\105\110\116\101\110\097\110\099\101 \109\111\100\101",C+1)sleep(2)error("\084\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100 \102\111\114 \099\111\110\102\105\103 \117\115\101\114")elseif t=="\103\114\097\110\116\101\100"then m("\065\067\067\069\083\083 \071\082\065\078\084\069\068",C-2)m("\047\047 \083\069\067\085\082\073\084\089 \067\072\069\067\075\080\079\073\078\084 \071\082\065\078\084\069\068 \092",C)m("\076\101\118\101\108\058 "..o,C+1)m("\087\101\108\099\111\109\101\033",C+2)j()else m("\065\067\067\069\083\083 \068\069\078\073\069\068",C-1)m("\073\110\115\117\102\102\105\099\105\101\110\116 \076\101\118\101\108\058 "..o,C+1)m("\082\101\113\117\105\114\101\100\058 "..(requiredMinimumLevel.."\043"),C+2)end end y()while true do local w,o,C,t=s("\109\111\110\105\116\111\114\095\116\111\117\099\104")if w=="\109\111\110\105\116\111\114\095\116\111\117\099\104"then H()local w=X()print("\069\109\112\108\111\121\101\101 \073\068 \101\110\116\101\114\101\100\058 "..w)rednet.send(SERVER_ID,w)local o,C=rednet.receive(5)if C then if C:find("\065\085\084\072\095\083\085\067\067\069\083\083\058")then local o=C:match("\065\085\084\072\095\083\085\067\067\069\083\083\058\040\046\043\041")b(w,o)sleep(3)y()elseif C=="\065\085\084\072\095\068\069\078\073\069\068"then K.clear()m("\065\067\067\069\083\083 \068\069\078\073\069\068",math.floor(F/2)-1)m("\073\110\118\097\108\105\100 \069\109\112\108\111\121\101\101 \073\068",math.floor(F/2)+1)sleep(3)y()else K.clear()m("\065\085\084\072\069\078\084\073\067\065\084\073\079\078 \070\065\073\076\069\068",math.floor(F/2)-1)m("\080\108\101\097\115\101 \116\114\121 \097\103\097\105\110",math.floor(F/2)+1)sleep(3)y()end else K.clear()m("\078\079 \082\069\083\080\079\078\083\069",math.floor(F/2)-1)m("\084\114\121 \097\103\097\105\110",math.floor(F/2)+1)sleep(3)y()end end end
