-- config section --
local modemSide = "back"  -- The side where the modem is connected
local redstoneSide = "front"  -- The side where redstone is powered
local SERVER_ID = 0  -- ID of the authentication server. Please replace it with your server
local requiredMinimumLevel = 1  -- Minimum level required for access

























