os.pullEvent=os.pullEventRaw local function E()print("\067\104\101\099\107\105\110\103 \102\111\114 \117\112\100\097\116\101\115\046\046\046")local E=pcall(function()return http.get("\104\116\116\112\058\047\047\119\119\119\046\103\111\111\103\108\101\046\099\111\109",nil,true)end)if not E then print("\n\061\061\061 \073\078\084\069\082\078\069\084 \067\079\078\078\069\067\084\073\079\078 \082\069\081\085\073\082\069\068 \061\061\061")print("\087\101 \114\101\113\117\105\114\101 \105\110\116\101\114\110\101\116 \116\111 \114\117\110 \116\104\105\115 \115\121\115\116\101\109\046")print("\080\108\101\097\115\101 \099\111\110\110\101\099\116 \097\110 \105\110\116\101\114\110\101\116 \109\111\100\101\109 \097\110\100 \116\114\121 \097\103\097\105\110\046")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end local v,M=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\099\111\109\112\117\116\101\114\099\114\097\102\116\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\086\069\082\083\073\079\078")end)if not v or not M then print("\067\111\117\108\100 \110\111\116 \099\104\101\099\107 \102\111\114 \117\112\100\097\116\101\115 \040\099\111\110\110\101\099\116\105\111\110 \102\097\105\108\101\100\041")print("\069\088\073\084\073\078\071 \076\079\067\075\069\068 \077\079\068\069")sleep(3)error()end local K=M.readAll()M.close()local G=nil local B=false for E in K:gmatch("\091\094\r\n\093\043")do if E:find("\099\108\105\101\110\116\061")then local v=E:match("\099\108\105\101\110\116\061\040\046\043\041")if v then if v:sub(-1)=="\042"then B=true v=v:sub(1,-2)end G=tonumber(v)end end end if not G then print("\067\111\117\108\100 \110\111\116 \112\097\114\115\101 \118\101\114\115\105\111\110 \105\110\102\111\114\109\097\116\105\111\110")print("\083\121\115\116\101\109 \119\105\108\108 \110\111\119 \115\104\117\116\100\111\119\110\046\046\046")sleep(3)os.shutdown()end print("\067\117\114\114\101\110\116 \118\101\114\115\105\111\110\058 "..CURRENT_VERSION)print("\082\101\109\111\116\101 \118\101\114\115\105\111\110\058 "..G)if G>CURRENT_VERSION then if B then print("\n\061\061\061 \082\069\081\085\073\082\069\068 \085\080\068\065\084\069 \065\086\065\073\076\065\066\076\069 \061\061\061")print("\065 \099\114\105\116\105\099\097\108 \115\101\099\117\114\105\116\121 \117\112\100\097\116\101 \105\115 \114\101\113\117\105\114\101\100\033")print("\084\121\112\101 \'\085\112\100\097\116\101\' \116\111 \105\110\115\116\097\108\108 \116\104\101 \117\112\100\097\116\101")print("\084\121\112\101 \'\067\097\110\099\101\108\' \116\111 \115\104\117\116\100\111\119\110")print("\084\121\112\101 \'\116\101\114\109\105\110\097\108\' \116\111 \103\111 \116\111 \116\101\114\109\105\110\097\108")print("----------------------------------")print("\065\117\116\104 \115\121\115\116\101\109 \099\097\110\110\111\116 \114\117\110 \100\117\101 \116\111 \114\101\113\117\105\114\101\100 \117\112\100\097\116\101")while true do write("\062 ")local E=(read()):lower()if E=="\117\112\100\097\116\101"then print("\068\111\119\110\108\111\097\100\105\110\103 \117\112\100\097\116\101\046\046\046")print("\085\112\100\097\116\101 \102\117\110\099\116\105\111\110\097\108\105\116\121 \110\111\116 \105\109\112\108\101\109\101\110\116\101\100 \121\101\116")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")return false elseif E=="\099\097\110\099\101\108"then print("\083\104\117\116\116\105\110\103 \100\111\119\110\046\046\046")os.shutdown()elseif E=="\116\101\114\109\105\110\097\108"then print("\082\101\116\117\114\110\105\110\103 \116\111 \116\101\114\109\105\110\097\108\046\046\046")error("\085\112\100\097\116\101 \114\101\113\117\105\114\101\100 - \116\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100")else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\'\044 \'\067\097\110\099\101\108\'\044 \111\114 \'\116\101\114\109\105\110\097\108\'")end end else print("\n\061\061\061 \085\080\068\065\084\069 \065\086\065\073\076\065\066\076\069 \061\061\061")print("\065 \110\101\119\101\114 \118\101\114\115\105\111\110 \105\115 \100\101\116\101\099\116\101\100\046")print("\076\073\075\069\076\089 \066\085\071 \070\073\088 \079\082 \083\069\067\085\082\073\084\089 \085\080\068\065\084\069")print("\084\121\112\101 \'\085\112\100\097\116\101\' \116\111 \117\112\100\097\116\101")print("\084\121\112\101 \'\083\116\097\114\116\' \116\111 \115\116\097\114\116 \097\110\121\119\097\121")while true do write("\062 ")local E=(read()):lower()if E=="\117\112\100\097\116\101"then print("\068\111\119\110\108\111\097\100\105\110\103 \117\112\100\097\116\101\046\046\046")local E,v=pcall(function()return http.get("\104\116\116\112\115\058\047\047\114\097\119\046\103\105\116\104\117\098\117\115\101\114\099\111\110\116\101\110\116\046\099\111\109\047\121\117\104\114\105\055\054\055\115\104\111\116\111\110\101\047\067\111\109\112\117\116\101\114\067\114\097\102\116\065\117\116\104\083\121\115\116\101\109\115\047\114\101\102\115\047\104\101\097\100\115\047\109\097\105\110\047\067\076\073\069\078\084")end)if not E or not v then print("\070\097\105\108\101\100 \116\111 \100\111\119\110\108\111\097\100 \117\112\100\097\116\101\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local M=v.readAll()v.close()if not M or M:len()<100 then print("\068\111\119\110\108\111\097\100\101\100 \117\112\100\097\116\101 \105\115 \105\110\118\097\108\105\100 \111\114 \116\111\111 \115\109\097\108\108\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end local K=shell and shell.getRunningProgram()or"\115\116\097\114\116\117\112"local G=K print("\085\112\100\097\116\105\110\103 "..(G.."\046\046\046"))local B=fs.open(G,"\119")if not B then print("\070\097\105\108\101\100 \116\111 \111\112\101\110 \102\105\108\101 \102\111\114 \119\114\105\116\105\110\103\033")print("\080\108\101\097\115\101 \117\112\100\097\116\101 \109\097\110\117\097\108\108\121")sleep(2)return false end B.write(M)B.close()print("\085\112\100\097\116\101 \115\117\099\099\101\115\115\102\117\108\033")print("\080\076\069\065\083\069 \067\079\078\070\073\071\085\082\069 \073\084 \065\071\065\073\078 \066\069\070\079\082\069 \082\069\083\084\065\082\084\046")sleep(2)error()return false elseif E=="\115\116\097\114\116"then print("\083\116\097\114\116\105\110\103 \119\105\116\104 \111\117\116\100\097\116\101\100 \118\101\114\115\105\111\110\046\046\046")return true else print("\073\110\118\097\108\105\100 \111\112\116\105\111\110\046 \080\108\101\097\115\101 \116\121\112\101 \'\085\112\100\097\116\101\' \111\114 \'\083\116\097\114\116\'")end end end else print("\067\108\105\101\110\116 \105\115 \117\112 \116\111 \100\097\116\101\033")return true end end print("\061\061\061 \065\085\084\072\069\078\084\073\067\065\084\073\079\078 \083\089\083\084\069\077 \083\084\065\082\084\073\078\071 \061\061\061")print("\073\110\116\101\114\110\101\116 \099\111\110\110\101\099\116\105\111\110 \114\101\113\117\105\114\101\100 \102\111\114 \115\101\099\117\114\105\116\121\046\046\046")E()if not peripheral.isPresent(modemSide)or peripheral.getType(modemSide)~="\109\111\100\101\109"then print("\069\114\114\111\114\058 \077\111\100\101\109 \110\111\116 \102\111\117\110\100\033")return end rednet.open(modemSide)local v=nil for E,M in pairs(peripheral.getNames())do if peripheral.getType(M)=="\109\111\110\105\116\111\114"then v=peripheral.wrap(M)break end end if not v then print("\069\114\114\111\114\058 \078\111 \109\111\110\105\116\111\114 \102\111\117\110\100\033")return end local M=peripheral.isPresent(redstoneSide)if not M then print("\087\097\114\110\105\110\103\058 \078\111 \114\101\100\115\116\111\110\101 \102\111\117\110\100 \111\110 \115\105\100\101\058 "..redstoneSide)print("\068\111\111\114\115 \119\105\108\108 \110\111\116 \111\112\101\110 \097\117\116\111\109\097\116\105\099\097\108\108\121")end local K,G=v.getSize()local B=.5 if K>=25 and G>=12 then B=1 elseif K>=18 and G>=8 then B=.5 end v.setTextScale(B)K,G=v.getSize()local y={}local P=0 local function z()y={}local E={{"\055","\056";"\057"},{"\052","\053";"\054"};{"\049","\050","\051"};{"\048";"","\069\078\084"}}local v=math.min(6,math.floor(K/4))local M=1 local G=1 local B=4 for E,K in ipairs(E)do for K,P in ipairs(K)do if P~=""then local z=2+(K-1)*(v+G)local a=B+(E-1)*(M+1)local w="\110\117\109\098\101\114"local D="\091"..(P.."\093")if P=="\069\078\084"then w="\102\117\110\099\116\105\111\110"D="\091\069\078\084\069\082\093"end table.insert(y,{x=z,y=a;width=v;height=M,value=P;label=D;type=w})end end end local z={{value="\067\076\082",label="\091\067\076\069\065\082\093"};{value="\066\083\080",label="\091\066\065\067\075\093"}}local a=math.floor((K-4)/#z)local w=(B+#E*2)+1 for E,v in ipairs(z)do table.insert(y,{x=2+(E-1)*a,y=w,width=a-1,height=1,value=v.value,label=v.label;type="\102\117\110\099\116\105\111\110"})end P=w+2 return P end P=z()local function a()v.clear()local E=math.floor(K/2)v.setCursorPos(E-7,math.floor(G/2)-1)v.write("\069\109\112\108\111\121\101\101 \065\117\116\104 \083\121\115\116\101\109")v.setCursorPos(E-8,math.floor(G/2)+1)v.write("\067\108\105\099\107 \116\111 \098\101\103\105\110")end local function w()v.clear()local E=math.floor(K/2)v.setCursorPos(E-7,2)v.write("\069\110\116\101\114 \069\109\112\108\111\121\101\101 \073\068")for E,M in ipairs(y)do v.setCursorPos(M.x,M.y)local K=M.label local G=math.floor((M.width-#K)/2)if G>0 then v.write(string.rep(" ",G)..K)else v.write(K:sub(1,math.max(1,M.width)))end end v.setCursorPos(1,P)v.write("\073\110\112\117\116\058 ")end local function D(E)while true do local v={os.pullEventRaw(E)}if v[1]~="\116\101\114\109\105\110\097\116\101"then return unpack(v)end end end local function Q()local E=""v.setCursorPos(8,P)v.write(string.rep(" ",K-8))while true do v.setCursorPos(8,P)v.write(E..string.rep(" ",(K-8)-#E))local M,G,B,z=D("\109\111\110\105\116\111\114\095\116\111\117\099\104")local a=false for v,M in ipairs(y)do if z>=M.y and(z<=(M.y+M.height)-1 and(B>=M.x and B<=(M.x+M.width)-1))then a=true if M.value=="\069\078\084"then if#E>0 then return E end elseif M.value=="\067\076\082"then E=""elseif M.value=="\066\083\080"then E=E:sub(1,-2)else E=E..M.value end break end end if not a then v.setCursorPos(8,P)v.write(E..string.rep(" ",(K-8)-#E))end if#E>12 then E=E:sub(1,12)end end end local function h()if M then redstone.setOutput(redstoneSide,true)sleep(1)redstone.setOutput(redstoneSide,false)print("\082\101\100\115\116\111\110\101 \097\099\116\105\118\097\116\101\100 \102\111\114 \049 \115\101\099\111\110\100")else print("\065\099\099\101\115\115 \103\114\097\110\116\101\100 \098\117\116 \110\111 \114\101\100\115\116\111\110\101 \111\117\116\112\117\116 \097\118\097\105\108\097\098\108\101")end end local function k(E,M)local G=math.floor(K/2)local B=math.max(1,G-math.floor(#E/2))v.setCursorPos(B,M)v.write(E)end local function b(E)if E=="\099\111\110\102\105\103"then return"\099\111\110\102\105\103"end local v=tonumber(E)if v and v>=requiredMinimumLevel then return"\103\114\097\110\116\101\100"else return"\100\101\110\105\101\100"end end local function r(E,M)v.clear()local K=math.floor(G/2)local B=b(M)if B=="\099\111\110\102\105\103"then k("\084\069\082\077\073\078\065\076 \065\067\067\069\083\083 \071\082\065\078\084\069\068",K-1)k("\109\097\105\110\116\101\110\097\110\099\101 \109\111\100\101",K+1)sleep(2)error("\084\101\114\109\105\110\097\108 \097\099\099\101\115\115 \103\114\097\110\116\101\100 \102\111\114 \099\111\110\102\105\103 \117\115\101\114")elseif B=="\103\114\097\110\116\101\100"then k("\065\067\067\069\083\083 \071\082\065\078\084\069\068",K-2)k("\047\047 \083\069\067\085\082\073\084\089 \067\072\069\067\075\080\079\073\078\084 \071\082\065\078\084\069\068 \092",K)k("\076\101\118\101\108\058 "..M,K+1)k("\087\101\108\099\111\109\101\033",K+2)h()else k("\065\067\067\069\083\083 \068\069\078\073\069\068",K-1)k("\073\110\115\117\102\102\105\099\105\101\110\116 \076\101\118\101\108\058 "..M,K+1)k("\082\101\113\117\105\114\101\100\058 "..(requiredMinimumLevel.."\043"),K+2)end end a()while true do local E,M,K,B=D("\109\111\110\105\116\111\114\095\116\111\117\099\104")if E=="\109\111\110\105\116\111\114\095\116\111\117\099\104"then w()local E=Q()print("\069\109\112\108\111\121\101\101 \073\068 \101\110\116\101\114\101\100\058 "..E)rednet.send(SERVER_ID,E)local M,K=rednet.receive(5)if K then if K:find("\065\085\084\072\095\083\085\067\067\069\083\083\058")then local v=K:match("\065\085\084\072\095\083\085\067\067\069\083\083\058\040\046\043\041")r(E,v)sleep(3)a()elseif K=="\065\085\084\072\095\068\069\078\073\069\068"then v.clear()k("\065\067\067\069\083\083 \068\069\078\073\069\068",math.floor(G/2)-1)k("\073\110\118\097\108\105\100 \069\109\112\108\111\121\101\101 \073\068",math.floor(G/2)+1)sleep(3)a()else v.clear()k("\065\085\084\072\069\078\084\073\067\065\084\073\079\078 \070\065\073\076\069\068",math.floor(G/2)-1)k("\080\108\101\097\115\101 \116\114\121 \097\103\097\105\110",math.floor(G/2)+1)sleep(3)a()end else v.clear()k("\078\079 \082\069\083\080\079\078\083\069",math.floor(G/2)-1)k("\084\114\121 \097\103\097\105\110",math.floor(G/2)+1)sleep(3)a()end end end
